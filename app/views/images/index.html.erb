<% provide(:title, "images") %>
<%= render 'layouts/header' %>
<div class="row">
<%= render 'layouts/sidebar' %>
		<div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 text-right">
			<% @images.each do |image| %>
				<ul>
					<li><%= image.title %></li>
					<li><%= image.date %></li>
					<li><%= image.dims %></li>
					<li><%= image.medium %></li>
				</ul>
			<% end %>
		</div>
</div>
<%= render 'layouts/footer' %>

